<h1>Listing users_locations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_locations.each do |users_location| %>
      <tr>
        <td><%= link_to 'Show', users_location %></td>
        <td><%= link_to 'Edit', edit_users_location_path(users_location) %></td>
        <td><%= link_to 'Destroy', users_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users location', new_users_location_path %>
